{
  "name": "starter-github-html-tests-travis-netlify",
  "description": "starter-github-html-tests-travis-netlify",
  "version": "1.0.0",
  "author": "jaffamonkey <paullittlebury@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "assert": "^1.4.1",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chromedriver": "^2.46.0",
    "eslint": "latest",
    "express": "latest",
    "geckodriver": "^1.16.1",
    "jasmine-json-test-reporter": "latest",
    "jest": "latest",
    "mocha": "latest",
    "nightmare": "^2.9.1",
    "nightwatch": "^1.0.19",
    "nightwatch-api": "latest",
    "nightwatch-cucumber": "9.1.2",
    "protractor": "5.4.1",
    "puppeteer": "latest",
    "request": "latest",
    "selenium-standalone": "^6.16.0",
    "selenium-webdriver": "^3.5.0",
    "superagent": "latest",
    "wdio-mocha-framework": "^0.6.4",
    "wdio-selenium-standalone-service": "0.0.12",
    "wdio-spec-reporter": "^0.1.5",
    "webdriverio": "^4.14.1"
  },
  "scripts": {
    "startapi": "node ./api/api.js &",
    "startweb": "node ./web/server.js &",
    "wdupdate": "./node_modules/.bin/webdriver-manager update",
    "test:api": "node ./tests/API/api-test.js",
    "test:superagent": "node ./tests/API/api-test-superagent-assert.js",
    "test:nightmare": "./node_modules/.bin/mocha --recursive ./tests/UI/electron-nightmare-mocha.js",
    "test:chai": "./node_modules/.bin/jest ./tests/UI/webdriver-chai-jest.js",
    "test:nightwatchapi": "node ./tests/UI/webdriver-nightwatchapi.js --config ./conf/nightwatch.conf.js -e chrome",
    "test:nightwatch": "./node_modules/.bin/nightwatch --test ./tests/UI/webdriver-nightwatch.js --config ./conf/nightwatch.conf.js -e chrome",
    "test:javascript": "node ./tests/UI/webdriver-javascript.js",
    "test:webdriverio": "node ./tests/UI/webdriverio-selenium.js",
    "test:webdriver": "node ./tests/UI/webdriver-node-async.js",
    "test:protractor": "./node_modules/.bin/protractor ./conf/protractor.conf.js",
    "lint": "./node_modules/.bin/eslint ./tests/**/**/*.js",
    "test:mocha": "mocha ./conf/puppeteer.conf.js --recursive ./tests/UI/puppeteer-mocha.js"
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/tests/UI/*jest.js"
    ]
  }
}
